{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
  },
  "globals": {
    "window": true,
    "document": true,
    "it": true,
    "describe": true,
    "expect": true,
  },
  "rules": {},
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./config/webpack.dev.config.js",
        "extensions": [".js", ".jsx"]
      }
    }
  }
}